{
	"restApiRoot": "/",
	"host": "127.0.0.1",
	"port": 3000,
	"cookieSecret": "---random---",
	"sessionSecret": "---random---",
	"remoting": {
		"context": false,
		"rest": {
			"normalizeHttpPath": false,
			"xml": false
		},
		"json": {
			"strict": false,
			"limit": "100kb"
		},
		"urlencoded": {
			"extended": true,
			"limit": "100kb"
		},
		"cors": false,
		"errorHandler": {
			"disableStackTrace": false
		}
	},
	"legacyExplorer": false
}